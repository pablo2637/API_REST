<div id="divTablaServicios">
    <h2 id="h2Servicios">Servicios: <%- servicios.length %> </h2>
    <a class="aDash" href="/dashboard/servicios/new">Nuevo Servicio <i class="fa-solid fa-plus"></i></a>

    <table>
        <thead>
            <tr>
                <th>SERVICIO</th>
                <th>DESCRIPCIÃ“N</th>
                <th>FECHA ALTA</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            <% servicios.forEach(serv=> { %>
                <tr class="serviciosDash">
                    <td> <%- serv.servicio %></td>
                    <td> <%- serv.descripcion %></td>
                    <td> <%- serv.fecha %></td>
                    <td class="btnsTablaDash">
                        <a href="/dashboard/servicios/id/<%- serv._id %>"><i class="fa-solid fa-pen-to-square"></i></a>
                        <a href=""><i class="fa-solid fa-eye"></i></a>
                        <a href="/dashboard/servicios/dlt/<%- serv._id %>"><i class="fa-solid fa-trash-can"></i></a>
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</div>